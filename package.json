{
    "name": "ng-circle-countdown",
    "version": "1.0.2",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "build_lib": "ng build ng-circle-countdown",
        "copy-license": "copy ./LICENSE ./dist/ng-circle-countdown",
        "copy-readme": "copy ./README.md ./dist/ng-circle-countdown",
        "copy-files": "npm run copy-license && npm run copy-readme",
        "create-link": "cd dist/ng-circle-countdown && npm link",
        "npm_pack": "npm run copy-files && cd dist/ng-circle-countdown && npm pack",
        "package": "npm run build_lib && npm run npm_pack",
        "watch": "ng build --watch --configuration development",
        "test": "jest --coverage",
        "test-ci": "jest --runInBand"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^16.0.0",
        "@angular/common": "^16.0.0",
        "@angular/compiler": "^16.0.0",
        "@angular/core": "^16.0.0",
        "@angular/forms": "^16.0.0",
        "@angular/platform-browser": "^16.0.0",
        "@angular/platform-browser-dynamic": "^16.0.0",
        "@angular/router": "^16.0.0",
        "rxjs": "~7.8.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.13.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^16.0.3",
        "@angular/cli": "~16.0.3",
        "@angular/compiler-cli": "^16.0.0",
        "@types/jest": "^29.5.8",
        "eslint": "^8.51.0",
        "jest": "^29.7.0",
        "jest-preset-angular": "^13.1.4",
        "ng-packagr": "^16.2.0",
        "prettier": "^3.0.1",
        "typescript": "~5.0.2"
    }
}
